            "data": {
              "type": "tm.Actor",
              "name": "User",
              "description": "",
              "outOfScope": false,
              "reasonOutOfScope": "",
              "providesAuthentication": false,
              "hasOpenThreats": false,
              "threats": [
                {
                  "id": "50e1db26-b3bc-437a-9396-205dd5c571f1",
                  "title": "User Spoofing",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Spoofing",
                  "description": "Users come to events in place of someone else.",
                  "mitigation": "User ratings by other users and the possibility to report unexpected behaviors of other users. Secure user authentication with OAuth so that only the people that are supposed to know the details of events are able to get them.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 1,
                  "score": ""
                },
                {
                  "id": "62d62f28-01b3-4f1d-aa95-65d95f42884a",
                  "title": "User Repudiation",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Repudiation",
                  "description": "Users refuse to admit they have come or not come to an event organized by them. They might also assault other users during the event and not admit to it.",
                  "mitigation": "Implement user ratings by other users so that customers of the app can know if their workout companion is to be trusted. Enable users to report unwanted behaviors so that with many reports the actual course of action can be established.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 2,
                  "score": ""
                }
              ]
            }
          },
