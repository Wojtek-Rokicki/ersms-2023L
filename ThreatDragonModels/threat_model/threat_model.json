            "data": {
              "type": "tm.Process",
              "name": "Mobile\nApp",
              "description": "",
              "outOfScope": false,
              "reasonOutOfScope": "",
              "privilegeLevel": "",
              "hasOpenThreats": false,
              "threats": [
                {
                  "id": "f2a35812-b06d-4960-9b0e-3240695bf53d",
                  "title": "App Spoofing",
                  "status": "NotApplicable",
                  "severity": "Low",
                  "type": "Spoofing",
                  "description": "An attacker gets access to the user's phone and acts as the user.",
                  "mitigation": "Not a threat we can act against. The user is responsible for their phone.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 3,
                  "score": ""
                },
                {
                  "id": "b5f8ab2b-4abd-48e3-af56-57efd73a5ddb",
                  "title": "App Repudiation",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Repudiation",
                  "description": "Users refusing to admit they have created/edited/deleted an event.\n",
                  "mitigation": "Store and maintain audit logs to establish a reliable trail of evidence.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 4,
                  "score": ""
                },
                {
                  "id": "5f43f365-cf7d-4cef-a59f-aabf532fb564",
                  "title": "App Tampering",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Tampering",
                  "description": "An attacker gets access to the user's phone and tampers with the data on their phone.\n",
                  "mitigation": "Store users' data concerning the app on the server so that there is always a copy the system can trust. What is more, there is no sensitive user data stored within the app.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 5,
                  "score": ""
                },
                {
                  "id": "b9f31116-e82d-40a8-9319-97fe1c309c96",
                  "title": "App Information Disclosure",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Information disclosure",
                  "description": "An attacker gets access to confidential data within the app on a user's phone.",
                  "mitigation": "No sensitive data is stored in the app. To avoid that the app uses the OAuth protocol to authenticate users. Additionally, the communication with the API uses TLS.",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 6,
                  "score": ""
                },
                {
                  "id": "7ce15683-e80e-4535-812c-fdde52f475ba",
                  "title": "App Elevation of privilege",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Elevation of privilege",
                  "description": "An attacker elevates their access through the app.",
                  "mitigation": "Implement the principle of least privilege. Strong authentication and authorization carefully implement authorization with OAuth. Implement comprehensive auditing and logging mechanisms to monitor user activities and detect any suspicious or unauthorized attempts to elevate privileges. ",
                  "modelType": "STRIDE",
                  "new": false,
                  "number": 18,
                  "score": ""
                }
              ]
            }
